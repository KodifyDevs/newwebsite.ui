{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@containers/*": [
        "./src/containers/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}